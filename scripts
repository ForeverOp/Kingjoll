local function label(y, text, color, bold)
    local l = Instance.new("TextLabel", frame)
    l.Position = UDim2.new(0,20,0,y)
    l.Size = UDim2.new(1,-40,0,40)
    l.BackgroundTransparency = 1
    l.TextXAlignment = Enum.TextXAlignment.Left
    l.Font = bold and Enum.Font.GothamBold or Enum.Font.Gotham
    l.TextSize = 16
    l.TextColor3 = color
    l.Text = text
    return l
end

label(10,"ðŸ›¡ï¸ Anti AFK Monitor",Color3.new(1,1,1),true)
local playLbl = label(70,"",Color3.fromRGB(200,200,200))
local afkLbl = label(120,"",Color3.fromRGB(255,120,120))
local statusLbl = label(180,"",Color3.fromRGB(120,255,120),true)

local totalTime, afkTime = 0, 0
local lastMove = os.clock()

local function fmt(t)
    return string.format("%02d:%02d:%02d",
        math.floor(t/3600),
        math.floor((t%3600)/60),
        math.floor(t%60)
    )
end

UserInputService.InputBegan:Connect(function()
    lastMove = os.clock()
    afkTime = 0
end)

RunService.RenderStepped:Connect(function(dt)
    if not AntiAFK then return end
    totalTime += dt

    if os.clock() - lastMove >= 5 then
        afkTime += dt
        statusLbl.Text = "Status: AFK"
        statusLbl.TextColor3 = Color3.fromRGB(255,120,120)
    else
        statusLbl.Text = "Status: Jogando"
        statusLbl.TextColor3 = Color3.fromRGB(120,255,120)
    end

    playLbl.Text = "ðŸŽ® Tempo jogando: "..fmt(totalTime)
    afkLbl.Text = "â¸ï¸ Tempo AFK: "..fmt(afkTime)
end)

-- ====================================
-- LOW GRAPHICS
-- ====================================
local function setLowGraphics(on)
    if not on then return end
    Lighting.GlobalShadows = false
    Lighting.Brightness = 1
    Lighting.FogEnd = 9e9

    for _,v in ipairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") then
            v.Material = Enum.Material.Plastic
            v.Reflectance = 0
        elseif v:IsA("Decal") or v:IsA("Texture") then
            v.Transparency = 1
        elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v.Enabled = false
        end
    end
end

-- ====================================
-- ACHAR MURDER
-- ====================================
local function getMurder()
    for _,p in ipairs(Players:GetPlayers()) do
        if p ~= LocalPlayer and p.Character then
            local bp = p:FindFirstChild("Backpack")
            if (bp and bp:FindFirstChild("Knife")) or p.Character:FindFirstChild("Knife") then
                return p
            end
        end
    end
end

-- ====================================
-- LOCK
-- ====================================
local LockEnabled = false
RunService.RenderStepped:Connect(function()
    if not LockEnabled then return end
    local m = getMurder()
    if m and m.Character and m.Character:FindFirstChild("HumanoidRootPart") then
        Camera.CFrame = CFrame.new(Camera.CFrame.Position, m.Character.HumanoidRootPart.Position)
    end
end)

-- ====================================
-- ESP
-- ====================================
local ESPEnabled = false
local ESP = {}

local function role(p)
    local ch, bp = p.Character, p:FindFirstChild("Backpack")
    if ch and (ch:FindFirstChild("Knife") or (bp and bp:FindFirstChild("Knife"))) then
        return "Murder", Color3.fromRGB(255,0,0)
    end
    if ch and (ch:FindFirstChild("Gun") or (bp and bp:FindFirstChild("Gun"))) then
        return "Sheriff", Color3.fromRGB(255,255,0)
    end
    return "Inocente", Color3.fromRGB(255,255,255)
end

local function clearESP(p)
    if ESP[p] then
        for _,v in ipairs(ESP[p]) do v:Destroy() end
        ESP[p] = nil
    end
end

local function applyESP(p)
    if not ESPEnabled or p == LocalPlayer or not p.Character then return end
    clearESP(p)

    local r,c = role(p)
    local hrp = p.Character:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    ESP[p] = {}

    local h = Instance.new("Highlight", CoreGui)
    h.Adornee = p.Character
    h.FillColor = c
    h.FillTransparency = 0.4
    h.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    table.insert(ESP[p], h)

    local bb = Instance.new("BillboardGui", hrp)
    bb.Size = UDim2.new(0,200,0,40)
    bb.StudsOffset = Vector3.new(0,3,0)
    bb.AlwaysOnTop = true

    local t = Instance.new("TextLabel", bb)
    t.Size = UDim2.new(1,0,1,0)
    t.BackgroundTransparency = 1
    t.Text = p.Name.." ["..r.."]"
    t.TextColor3 = c
    t.TextStrokeTransparency = 0
    t.Font = Enum.Font.GothamBold
    t.TextSize = 14

    table.insert(ESP[p], bb)
end

RunService.RenderStepped:Connect(function()
    if ESPEnabled then
        for _,p in ipairs(Players:GetPlayers()) do
            applyESP(p)
        end
    end
end)

