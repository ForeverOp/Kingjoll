-- ================= PLAYER LIST =================
local PlayerList = {}
local PlayerIndex = 0
local Target
local Loop = false
local SavedCFrame

local function RefreshPlayers()
    PlayerList = {}
    for _,p in ipairs(Players:GetPlayers()) do
        if p ~= LP then
            table.insert(PlayerList, p)
        end
    end
end

RefreshPlayers()
Players.PlayerAdded:Connect(RefreshPlayers)
Players.PlayerRemoving:Connect(RefreshPlayers)

dropdown.MouseButton1Click:Connect(function()
    if #PlayerList == 0 then
        dropdown.Text = "Nenhum player"
        return
    end

    PlayerIndex += 1
    if PlayerIndex > #PlayerList then PlayerIndex = 1 end
    Target = PlayerList[PlayerIndex]
    dropdown.Text = "ðŸŽ¯ "..Target.Name
end)

-- ================= SAFE FLING =================
local function SafeFling()
    if not Target or not Target.Character then return end
    if not LP.Character then return end

    local char = LP.Character
    local hum = char:FindFirstChildOfClass("Humanoid")
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local thrp = Target.Character:FindFirstChild("HumanoidRootPart")
    if not hum or not hrp or not thrp then return end

    SavedCFrame = hrp.CFrame

    local oldHealth = hum.Health
    hum.Health = math.huge
    hum.PlatformStand = true

    for _,v in ipairs(char:GetDescendants()) do
        if v:IsA("BasePart") then
            v.CanCollide = false
            v.Massless = true
        end
    end

    local bav = Instance.new("BodyAngularVelocity", hrp)
    bav.AngularVelocity = Vector3.new(0,50000,0)
    bav.MaxTorque = Vector3.new(1e9,1e9,1e9)

    local bv = Instance.new("BodyVelocity", hrp)
    bv.MaxForce = Vector3.new(1e9,1e9,1e9)

    local t = tick()
    while tick() - t < 1.3 do
        if not thrp.Parent then break end
        hrp.CFrame = thrp.CFrame * CFrame.new(0,0,-1.2)
        bv.Velocity = (thrp.Position - hrp.Position).Unit * 220
        RunService.Heartbeat:Wait()
    end

    bav:Destroy()
    bv:Destroy()

    task.wait(0.05)
    if hrp and SavedCFrame then
        hrp.CFrame = SavedCFrame
    end

    hum.PlatformStand = false
    hum.Health = oldHealth
end

-- ================= LOOP =================
toggle.MouseButton1Click:Connect(function()
    Loop = not Loop
    toggle.Text = Loop and "ðŸ” Loop Fling: ON" or "ðŸ” Loop Fling: OFF"
    toggle.TextColor3 = Loop and Color3.fromRGB(100,255,100) or Color3.fromRGB(255,100,100)

    if Loop then
        task.spawn(function()
            while Loop do
                pcall(SafeFling)
                task.wait(0.35)
            end
        end)
    end
end)

