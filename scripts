    b.Font = Enum.Font.GothamBold
    b.TextSize = 22
    b.Active = false
    b.Draggable = false
    b.AutoButtonColor = true

    Instance.new("UICorner", b).CornerRadius = UDim.new(1,0)
    return g,b
end

